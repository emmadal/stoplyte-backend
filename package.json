  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "db:generate": "npx prisma db pull && ./replace_float.sh && npx prisma generate ",
    "start": "npx prisma generate && NODE_ENV=development nest start",
    "start:dev": "npx prisma generate && NODE_ENV=development nest start --watch",
    "start:debug": "npx prisma generate && NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "prisma generate --no-engine"
  },
